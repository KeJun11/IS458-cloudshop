# Copy this file to .env and update the values for your environment.

TF_VAR_project_name=aws-ecommerce
TF_VAR_env=dev
TF_VAR_region=us-east-1
# Populate with a verified email address before using SES (leave empty to skip SES setup)
TF_VAR_ses_sender_email=
# Optional: adjust the CloudFront default root object if your frontend uses a different entry point
TF_VAR_frontend_index_key=index.html
# Optional: number of days to retain generated invoices
TF_VAR_invoice_bucket_lifecycle_days=30
# Optional: provide additional tags as JSON (e.g. {"Owner":"team","CostCenter":"1234"})
TF_VAR_additional_tags={}
